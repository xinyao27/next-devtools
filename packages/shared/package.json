{
  "name": "@next-devtools/shared",
  "version": "0.3.0-beta.2",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/xinyao27/next-devtools",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinyao27/next-devtools.git",
    "directory": "packages/shared"
  },
  "author": {
    "name": "Xinyao",
    "email": "hi@xinyao.me"
  },
  "files": [
    "*.cjs",
    "*.d.ts",
    "*.js",
    "*.mjs",
    "*.mts",
    "dist"
  ],
  "exports": {
    "./components": {
      "require": {
        "types": "./dist/cjs/components.d.ts",
        "default": "./dist/cjs/components.js"
      },
      "import": {
        "types": "./dist/es/components.d.mts",
        "default": "./dist/es/components.mjs"
      }
    },
    "./constants": {
      "require": {
        "types": "./dist/cjs/constants.d.ts",
        "default": "./dist/cjs/constants.js"
      },
      "import": {
        "types": "./dist/es/constants.d.mts",
        "default": "./dist/es/constants.mjs"
      }
    },
    "./context": {
      "require": {
        "types": "./dist/cjs/context.d.ts",
        "default": "./dist/cjs/context.js"
      },
      "import": {
        "types": "./dist/es/context.d.mts",
        "default": "./dist/es/context.mjs"
      }
    },
    "./types": {
      "require": {
        "types": "./dist/cjs/types.d.ts",
        "default": "./dist/cjs/types.js"
      },
      "import": {
        "types": "./dist/es/types.d.mts",
        "default": "./dist/es/types.mjs"
      }
    },
    "./utils": {
      "require": {
        "types": "./dist/cjs/utils.d.ts",
        "default": "./dist/cjs/utils.js"
      },
      "import": {
        "types": "./dist/es/utils.d.mts",
        "default": "./dist/es/utils.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bunchee --external react",
    "dev": "bunchee --no-clean --external react --watch"
  },
  "peerDependencies": {
    "react": ">= 18"
  },
  "dependencies": {
    "date-fns": "catalog:",
    "microdiff": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:react19",
    "next": "catalog:next15",
    "schema-dts": "catalog:",
    "webpack": "catalog:",
    "zustand": "catalog:"
  }
}
